{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "flowtype",
        "jest"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "react/jsx-max-props-per-line": 0
    }
}